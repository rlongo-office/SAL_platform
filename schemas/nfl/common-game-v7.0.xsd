            <xs:element name="penalty" minOccurs="0" maxOccurs="1">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="team" type="teamType"/>
                    </xs:sequence>
                    <xs:attribute name="description" use="optional" type="xs:string"/>
                    <xs:attribute name="result" use="optional" type="xs:string"/>
                    <xs:attribute name="yards" use="optional" type="xs:string"/>
                    <xs:attribute name="no_play" use="optional" type="xs:string"/>
                </xs:complexType>
            </xs:element>
            <xs:element name="recovery" minOccurs="0" maxOccurs="1">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="team" type="teamType"/>
                    </xs:sequence>
                    <xs:attribute name="type" use="optional" type="xs:string"/>
                    <xs:attribute name="first_touch" use="optional" type="xs:string"/>
                </xs:complexType>
            </xs:element>
            <xs:element name="review" minOccurs="0" maxOccurs="1">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="team" minOccurs="0" maxOccurs="1" type="teamType"/>
                    </xs:sequence>
                    <xs:attribute name="type" use="optional" type="xs:string"/>
                    <xs:attribute name="result" use="optional" type="xs:string"/>
                    <xs:attribute name="reversed" use="optional" type="xs:string"/>
                </xs:complexType>
            </xs:element>
            <xs:element name="players" minOccurs="0" maxOccurs="1">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="player" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:attributeGroup ref="basePlayerAttributes"/>
                                <xs:attribute name="role" use="optional" type="xs:string"/>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attributeGroup ref="playDetailsAttributes" />
        <xs:attribute name="safety" type="xs:boolean"/>
    </xs:complexType>

    <xs:complexType name="extFieldLocationType">
        <xs:attribute name="alias" use="optional" type="xs:string"/>
        <xs:attribute name="yardline" use="optional" type="xs:integer"/>
    </xs:complexType>

    <xs:attributeGroup name="basePeriodAttributes">
        <xs:attribute name="id"          use="required" type="xs:string"/>
        <xs:attribute name="number"      use="required" type="xs:integer"/>
        <xs:attribute name="sequence"    use="required" type="xs:integer"/>
    </xs:attributeGroup>

    <xs:complexType name="gamePlayDetailsType">
        <xs:complexContent>
            <xs:extension base="basePlayType">
                <xs:sequence>
                    <xs:element name="drive-info" minOccurs="0" maxOccurs="1">
                        <xs:complexType>
                            <xs:attribute name="duration" use="optional" type="xs:string"/>
                            <xs:attribute name="net_yards" use="optional" type="xs:integer"/>
                            <xs:attribute name="play_count" use="optional" type="xs:integer"/>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="score" minOccurs="0" maxOccurs="1" type="playScoreType"/>
                    <xs:choice minOccurs="0" maxOccurs="1">
                        <xs:element name="quarter">
                            <xs:complexType>
                                <xs:attributeGroup ref="basePeriodAttributes" />
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="overtime">
                            <xs:complexType>
                                <xs:attributeGroup ref="basePeriodAttributes" />
                            </xs:complexType>
                        </xs:element>
                    </xs:choice>
                    <xs:element name="statistics" minOccurs="0" maxOccurs="1" type="extPlayStatisticsType"/>
                    <xs:element name="details" minOccurs="0" maxOccurs="1">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="detail" maxOccurs="unbounded" type="playDetailsType"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
                <xs:attributeGroup ref="extPlayAttributes" />
                <xs:attribute name="play_action" type="xs:boolean"/>
                <xs:attribute name="run_pass_option" type="xs:boolean"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="gameDetailsDriveType">
        <xs:sequence>
            <xs:element name="offensive_team" minOccurs="0" maxOccurs="1" type="driveTeamType"/>
            <xs:element name="defensive_team" minOccurs="0" maxOccurs="1" type="driveTeamType"/>
            <xs:choice maxOccurs="unbounded">
                <xs:element name="event" type="gameEventType"/>
                <xs:element name="play" type="gamePlayDetailsType"/>
            </xs:choice>
        </xs:sequence>
        <xs:attributeGroup ref="baseDriveAttributes" />
    </xs:complexType>

    <xs:attributeGroup name="coinTossAttributes">
        <xs:attribute name="outcome" use="required" type="xs:string"/>
        <xs:attribute name="decision" use="optional" type="xs:string"/>
        <xs:attribute name="direction" use="optional" type="xs:string"/>
    </xs:attributeGroup>

    <xs:complexType name="coinTossType">
        <xs:sequence>
            <xs:element name="home" minOccurs="1" maxOccurs="1">
                <xs:complexType>
                    <xs:attributeGroup ref="coinTossAttributes" />
                </xs:complexType>
            </xs:element>
            <xs:element name="away" minOccurs="1" maxOccurs="1">
                <xs:complexType>
                    <xs:attributeGroup ref="coinTossAttributes" />
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="quarter" use="optional" type="xs:string"/>
    </xs:complexType>

    <xs:complexType name="weatherType">
        <xs:sequence>
            <xs:element name="wind">
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="xs:string">
                            <xs:attribute type="xs:byte" name="speed"/>
                            <xs:attribute type="xs:string" name="direction"/>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attribute type="xs:string" name="condition"/>
        <xs:attribute type="xs:byte" name="humidity"/>
        <xs:attribute type="xs:byte" name="temp"/>
    </xs:complexType>
</xs:schema>
