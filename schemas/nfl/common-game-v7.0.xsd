<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">

    <xs:include schemaLocation="common-v7.0.xsd"/>

    <xs:attributeGroup name="outcomeAttributes">
        <xs:attribute name="outcome" use="optional">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="win" />
                    <xs:enumeration value="loss" />
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:attributeGroup>

    <xs:complexType name="summaryType">
        <xs:sequence>
            <xs:element name="season" minOccurs="1" maxOccurs="1">
                <xs:complexType>
                    <xs:attributeGroup ref="baseSeasonAttributes"/>
                </xs:complexType>
            </xs:element>
            <xs:element name="week" minOccurs="1" maxOccurs="1">
                <xs:complexType>
                    <xs:attributeGroup ref="baseWeekAttributes"/>
                </xs:complexType>
            </xs:element>
            <xs:element name="venue" minOccurs="0" maxOccurs="1" type="venueType"/>
            <xs:element name="home" minOccurs="1" maxOccurs="1" type="gameTeamType"/>
            <xs:element name="away" minOccurs="1" maxOccurs="1" type="gameTeamType"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="gameTeamType">
        <xs:complexContent>
            <xs:extension base="teamType">
                <xs:sequence>
                    <xs:element type="sourceType" name="source" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="record" minOccurs="0" maxOccurs="1">
                        <xs:complexType>
                            <xs:attribute name="wins" use="required" type="xs:integer"/>
                            <xs:attribute name="losses" use="required" type="xs:integer"/>
                            <xs:attribute name="ties" use="required" type="xs:integer"/>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="points"              use="optional" type="xs:integer"/>
                <xs:attribute name="remaining_timeouts"  use="optional" type="xs:integer"/>
                <xs:attribute name="used_timeouts"       use="optional" type="xs:integer"/>
                <xs:attribute name="remaining_challenges"  use="optional" type="xs:integer"/>
                <xs:attribute name="used_challenges"       use="optional" type="xs:integer"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="sourceType">
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attribute type="xs:string" name="id" use="optional"/>
                <xs:attribute type="xs:string" name="title" use="optional"/>
                <xs:attribute type="xs:string" name="status" use="optional"/>
                <xs:attribute type="xs:string" name="coverage" use="optional"/>
                <xs:attribute type="xs:string" name="game_type" use="optional"/>
                <xs:attribute type="xs:string" name="scheduled" use="optional"/>
                <xs:attribute type="xs:string" name="entry_mode" use="optional"/>
                <xs:attribute type="xs:string" name="outcome" use="optional"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:complexType name="timezonesType">
        <xs:attribute type="xs:string" name="venue" use="optional"/>
        <xs:attribute type="xs:string" name="home" use="optional"/>
        <xs:attribute type="xs:string" name="away" use="optional"/>
    </xs:complexType>

    <xs:complexType name="fieldSituationType">
        <xs:sequence>
            <xs:element name="possession" minOccurs="0" maxOccurs="1" type="teamType"/>
            <xs:element name="location" minOccurs="0" maxOccurs="1" type="fieldLocationType"/>
        </xs:sequence>
        <xs:attribute name="clock" use="required" type="xs:string"/>
        <xs:attribute name="down" use="optional" type="xs:integer"/>
        <xs:attribute name="yfd" use="required" type="xs:integer"/>
    </xs:complexType>

    <xs:complexType name="fieldLocationType">
        <xs:attributeGroup ref="baseTeamAttributes"/>
        <xs:attribute name="yardline" use="required" type="xs:integer"/>
    </xs:complexType>

    <xs:attributeGroup name="baseGameEventAttributes">
        <xs:attribute name="id" use="required" type="xs:string"/>
        <xs:attribute name="sequence" use="required" type="xs:decimal"/>
        <xs:attribute name="reference" use="optional" type="xs:integer"/>
        <xs:attribute name="clock" use="optional" type="xs:string"/>
        <xs:attribute name="wall_clock" use="optional" type="xs:dateTime"/>
        <xs:attribute name="deleted" use="optional" type="xs:boolean"/>
        <xs:attribute name="source" use="optional"/>
    </xs:attributeGroup>

    <xs:complexType name="gameEventType">
        <xs:sequence minOccurs="0">
            <xs:element name="description" minOccurs="0" maxOccurs="1" type="xs:string"/>
            <xs:element name="alt-description" minOccurs="0" maxOccurs="1" type="xs:string"/>
        </xs:sequence>
        <xs:attributeGroup ref="baseGameEventAttributes"/>
        <xs:attribute name="type" use="required">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="setup"/>
                    <xs:enumeration value="timeout"/>
                    <xs:enumeration value="tv_timeout"/>
                    <xs:enumeration value="comment"/>
                    <xs:enumeration value="two_minute_warning"/>
                    <xs:enumeration value="period_end"/>
                    <xs:enumeration value="game_over"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="created_at" use="optional" type="xs:string"/>
        <xs:attribute name="updated_at" use="optional" type="xs:string"/>
    </xs:complexType>

    <xs:attributeGroup name="basePlayAttributes">
        <xs:attribute name="created_at" use="optional" type="xs:string"/>
        <xs:attribute name="updated_at" use="optional" type="xs:string"/>
        <xs:attributeGroup ref="baseGameEventAttributes"/>
        <xs:attribute name="type" use="required">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="pass"/>
                    <xs:enumeration value="rush"/>
                    <xs:enumeration value="faircatch_kick"/>
                    <xs:enumeration value="extra_point"/>
                    <xs:enumeration value="conversion"/>
                    <xs:enumeration value="free_kick"/>
                    <xs:enumeration value="kickoff"/>
                    <xs:enumeration value="punt"/>
                    <xs:enumeration value="field_goal"/>
                    <xs:enumeration value="penalty"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>

    </xs:attributeGroup>

    <xs:complexType name="basePlayType">
        <xs:sequence>
            <xs:element name="start_situation" minOccurs="0" maxOccurs="1" type="fieldSituationType"/>
            <xs:element name="end_situation" minOccurs="0" maxOccurs="1" type="fieldSituationType"/>
            <xs:element name="description" minOccurs="0" maxOccurs="1" type="xs:string"/>
            <xs:element name="alt-description" minOccurs="0" maxOccurs="1" type="xs:string"/>
            <xs:element name="scoring-description" minOccurs="0" maxOccurs="1" type="xs:string"/>
        </xs:sequence>
        <xs:attributeGroup ref="basePlayAttributes" />
        <xs:attribute name="away_points" use="optional" type="xs:integer"/>
        <xs:attribute name="home_points" use="optional" type="xs:integer"/>
        <xs:attribute name="play_clock" use="optional" type="xs:integer"/>
        <xs:attribute name="scoring_play" use="optional" type="xs:boolean"/>
        <xs:attribute name="goaltogo" use="optional" type="xs:boolean"/>
    </xs:complexType>

    <xs:complexType name="gamePlayType">
        <xs:complexContent>
            <xs:extension base="basePlayType">
                <xs:sequence>
                    <xs:element name="drive-info" minOccurs="0" maxOccurs="1">
                        <xs:complexType>
                            <xs:attribute name="duration" use="optional" type="xs:string"/>
                            <xs:attribute name="net_yards" use="optional" type="xs:integer"/>
                            <xs:attribute name="play_count" use="optional" type="xs:integer"/>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="score" minOccurs="0" maxOccurs="1" type="playScoreType"/>
                    <xs:element name="statistics" minOccurs="0" maxOccurs="1" type="playStatisticsType"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:attributeGroup name="baseDriveAttributes">
        <xs:attribute name="id" use="required" type="xs:string"/>
        <xs:attribute name="sequence" use="required" type="xs:integer"/>
        <xs:attribute name="team_sequence" use="optional" type="xs:integer"/>
        <xs:attribute name="start_reason" use="optional" type="driveReason"/>
        <xs:attribute name="end_reason" use="optional" type="driveReason"/>
        <xs:attribute name="duration" use="optional" type="xs:string"/>
        <xs:attribute name="start_clock" use="optional" type="xs:string"/>
        <xs:attribute name="end_clock" use="optional" type="xs:string"/>
        <xs:attribute name="first_downs" use="required" type="xs:integer"/>
        <xs:attribute name="gain" use="optional" type="xs:integer"/>
        <xs:attribute name="inside_20" use="optional" type="xs:boolean"/>
        <xs:attribute name="penalty_yards" use="optional" type="xs:integer"/>
        <xs:attribute name="net_yards" use="optional" type="xs:integer"/>
        <xs:attribute name="play_count" use="required" type="xs:integer"/>
        <xs:attribute name="scoring_drive" use="optional" type="xs:boolean"/>
        <xs:attribute name="created_at" use="optional" type="xs:string"/>
        <xs:attribute name="updated_at" use="optional" type="xs:string"/>
        <xs:attribute name="first_drive_yardline" use="optional" type="xs:integer"/>
        <xs:attribute name="last_drive_yardline" use="optional" type="xs:integer"/>
        <xs:attribute name="farthest_drive_yardline" use="optional" type="xs:integer"/>
        <xs:attribute name="pat_successful" use="optional" type="xs:boolean"/>
        <xs:attribute name="pat_points_attempted" use="optional" type="xs:integer"/>
    </xs:attributeGroup>

    <xs:complexType name="driveTeamType">
        <xs:attribute name="id" use="required" type="xs:string"/>
        <xs:attribute name="points" use="optional" type="xs:integer"/>
    </xs:complexType>

    <xs:simpleType name="driveReason">
        <xs:restriction base="xs:string">
            <xs:enumeration value="UNKNOWN"/>
            <xs:enumeration value="Touchdown"/>
            <xs:enumeration value="Safety"/>
            <xs:enumeration value="Field Goal"/>
            <xs:enumeration value="Missed FG"/>
            <xs:enumeration value="Blocked FG"/>
            <xs:enumeration value="Blocked FG, Downs"/>
            <xs:enumeration value="Blocked FG, Safety"/>
            <xs:enumeration value="Punt"/>
            <xs:enumeration value="Blocked Punt"/>
            <xs:enumeration value="Blocked Punt, Downs"/>
            <xs:enumeration value="Blocked Punt, Safety"/>
            <xs:enumeration value="Downs"/>
            <xs:enumeration value="Interception"/>
            <xs:enumeration value="Fumble"/>
            <xs:enumeration value="Fumble, Safety"/>
            <xs:enumeration value="Muffed FG"/>
            <xs:enumeration value="Muffed Punt"/>
            <xs:enumeration value="Muffed Kickoff"/>
            <xs:enumeration value="Kickoff"/>
            <xs:enumeration value="Own Kickoff"/>
            <xs:enumeration value="Onside Kick"/>
            <xs:enumeration value="Kickoff, No Play"/>
            <xs:enumeration value="End of Half"/>
            <xs:enumeration value="End of Game"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="playScoreType">
        <xs:sequence>
            <xs:element name="points-after-play" minOccurs="0" maxOccurs="1">
                <xs:complexType>
                    <xs:attributeGroup ref="baseGameEventAttributes"/>
                    <xs:attribute name="type" use="required">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="extra_point"/>
                                <xs:enumeration value="conversion"/>
                                <xs:enumeration value="penalty"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="clock" use="required" type="xs:string"/>
        <xs:attribute name="sequence" use="required" type="xs:integer"/>

        <xs:attribute name="away_points" use="required" type="xs:integer"/>
        <xs:attribute name="home_points" use="required" type="xs:integer"/>
        <xs:attribute name="points" use="required" type="xs:integer"/>
    </xs:complexType>

    <xs:complexType name="playStatisticsType">
        <xs:choice maxOccurs="unbounded">
            <xs:element name="timeout">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="basePlayStatisticType">
                            <xs:attribute name="description" use="optional" type="xs:string"/>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="misc">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="basePlayStatisticType">
                            <xs:attribute name="yards" use="optional" type="xs:integer"/>
                            <xs:attribute name="touchdown" use="optional" type="xs:integer"/>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="pass">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="basePlayStatisticType">
                            <xs:attribute name="attempt" use="optional" type="xs:integer"/>
                            <xs:attribute name="complete" use="optional" type="xs:integer"/>
                            <xs:attribute name="yards" use="optional" type="xs:integer"/>
                            <xs:attribute name="att_yards" use="optional" type="xs:integer"/>
                            <xs:attribute name="interception" use="optional" type="xs:integer"/>
                            <xs:attribute name="firstdown" use="optional" type="xs:integer"/>
                            <xs:attribute name="touchdown" use="optional" type="xs:integer"/>
                            <xs:attribute name="sack" use="optional" type="xs:integer"/>
                            <xs:attribute name="sack_yards" use="optional" type="xs:double"/>
                            <xs:attribute name="inside_20" use="optional" type="xs:integer"/>
                            <xs:attribute name="goaltogo" use="optional" type="xs:integer"/>
                            <xs:attribute name="safety" use="optional" type="xs:integer"/>
                            <xs:attribute name="int_touchdown" use="optional" type="xs:integer"/>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="receive">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="basePlayStatisticType">
                            <xs:attribute name="firstdown" use="optional" type="xs:integer"/>
                            <xs:attribute name="touchdown" use="optional" type="xs:integer"/>
                            <xs:attribute name="safety" use="optional" type="xs:integer"/>
                            <xs:attribute name="target" use="optional" type="xs:integer"/>
                            <xs:attribute name="reception" use="optional" type="xs:integer"/>
                            <xs:attribute name="yards" use="optional" type="xs:integer"/>
                            <xs:attribute name="yards_after_catch" use="optional" type="xs:integer"/>
                            <xs:attribute name="fumble" use="optional" type="xs:integer"/>
                            <xs:attribute name="dropped" use="optional" type="xs:integer"/>
                            <xs:attribute name="inside_20" use="optional" type="xs:integer"/>
                            <xs:attribute name="goaltogo" use="optional" type="xs:integer"/>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="rush">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="basePlayStatisticType">
                            <xs:attribute name="attempt" use="optional" type="xs:integer"/>
                            <xs:attribute name="yards" use="optional" type="xs:integer"/>
                            <xs:attribute name="touchdown" use="optional" type="xs:integer"/>
                            <xs:attribute name="firstdown" use="optional" type="xs:integer"/>
                            <xs:attribute name="safety" use="optional" type="xs:integer"/>
                            <xs:attribute name="fumble" use="optional" type="xs:integer"/>
                            <xs:attribute name="inside_20" use="optional" type="xs:integer"/>
                            <xs:attribute name="goaltogo" use="optional" type="xs:integer"/>
                            <xs:attribute name="lateral" use="optional" type="xs:integer"/>
                            <xs:attribute name="tlost" use="optional" type="xs:integer"/>
                            <xs:attribute name="tlost_yards" use="optional" type="xs:integer"/>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="kick">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="basePlayStatisticType">
                            <xs:attribute name="attempt" use="optional" type="xs:integer"/>
                            <xs:attribute name="yards" use="optional" type="xs:integer"/>
                            <xs:attribute name="net_yards" use="optional" type="xs:integer"/>
                            <xs:attribute name="gross_yards" use="optional" type="xs:integer"/>
                            <xs:attribute name="inside_20" use="optional" type="xs:integer"/>
                            <xs:attribute name="endzone" use="optional" type="xs:integer"/>
                            <xs:attribute name="touchback" use="optional" type="xs:integer"/>
                            <xs:attribute name="own_rec" use="optional" type="xs:integer"/>
                            <xs:attribute name="own_rec_td" use="optional" type="xs:integer"/>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="punt">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="basePlayStatisticType">
                            <xs:attribute name="attempt" use="optional" type="xs:integer"/>
                            <xs:attribute name="yards" use="optional" type="xs:integer"/>
                            <xs:attribute name="net_yards" use="optional" type="xs:integer"/>
                            <xs:attribute name="inside_20" use="optional" type="xs:integer"/>
                            <xs:attribute name="endzone" use="optional" type="xs:integer"/>
                            <xs:attribute name="touchback" use="optional" type="xs:integer"/>
                            <xs:attribute name="blocked" use="optional" type="xs:integer"/>
                            <xs:attribute name="safety" use="optional" type="xs:integer"/>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="field_goal">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="basePlayStatisticType">
                            <xs:attribute name="attempt" use="optional" type="xs:integer"/>
                            <xs:attribute name="att_yards" use="optional" type="xs:integer"/>
                            <xs:attribute name="yards" use="optional" type="xs:integer"/>
                            <xs:attribute name="missed" use="optional" type="xs:integer"/>
                            <xs:attribute name="blocked" use="optional" type="xs:integer"/>
                            <xs:attribute name="returned" use="optional" type="xs:integer"/>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="extra_point">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="basePlayStatisticType">
                            <xs:attribute name="attempt" use="optional" type="xs:integer"/>
                            <xs:attribute name="missed" use="optional" type="xs:integer"/>
                            <xs:attribute name="blocked" use="optional" type="xs:integer"/>
                            <xs:attribute name="returned" use="optional" type="xs:integer"/>
                            <xs:attribute name="safety" use="optional" type="xs:integer"/>
                            <xs:attribute name="aborted" use="optional" type="xs:integer"/>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="penalty">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="basePlayStatisticType">
                            <xs:attribute name="penalty" use="optional" type="xs:integer"/>
                            <xs:attribute name="yards" use="optional" type="xs:integer"/>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="fumble">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="basePlayStatisticType">
                            <xs:attribute name="fumble" use="optional" type="xs:integer"/>
                            <xs:attribute name="forced" use="optional" type="xs:integer"/>
                            <xs:attribute name="out_of_bounds" use="optional" type="xs:integer"/>
                            <xs:attribute name="own_rec" use="optional" type="xs:integer"/>
                            <xs:attribute name="opp_rec" use="optional" type="xs:integer"/>
                            <xs:attribute name="own_rec_yards" use="optional" type="xs:integer"/>
                            <xs:attribute name="opp_rec_yards" use="optional" type="xs:integer"/>
                            <xs:attribute name="own_rec_td" use="optional" type="xs:integer"/>
                            <xs:attribute name="opp_rec_td" use="optional" type="xs:integer"/>
                            <xs:attribute name="lost" use="optional" type="xs:integer"/>
                            <xs:attribute name="play_category" use="optional" type="xs:string"/>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="return">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="basePlayStatisticType">
                            <xs:attribute name="return" use="optional" type="xs:integer"/>
                            <xs:attribute name="yards" use="optional" type="xs:integer"/>
                            <xs:attribute name="touchdown" use="optional" type="xs:integer"/>
                            <xs:attribute name="firstdown" use="optional" type="xs:integer"/>
                            <xs:attribute name="faircatch" use="optional" type="xs:integer"/>
                            <xs:attribute name="out_of_bounds" use="optional" type="xs:integer"/>
                            <xs:attribute name="downed" use="optional" type="xs:integer"/>
                            <xs:attribute name="touchback" use="optional" type="xs:integer"/>
                            <xs:attribute name="lateral" use="optional" type="xs:integer"/>
                            <xs:attribute name="category" use="optional">
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="punt_return" />
                                        <xs:enumeration value="kick_return" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:attribute>
                            <xs:attribute name="play_category" use="optional" type="xs:string"/>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="block">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="basePlayStatisticType">
                            <xs:attribute name="block" use="optional" type="xs:integer"/>
                            <xs:attribute name="category" use="optional">
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="field_goal" />
                                        <xs:enumeration value="extra_point" />
                                        <xs:enumeration value="punt" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:attribute>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="defense">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="basePlayStatisticType">
                            <xs:attribute name="tackle" use="optional" type="xs:integer"/>
                            <xs:attribute name="ast_tackle" use="optional" type="xs:integer"/>
                            <xs:attribute name="primary" use="optional" type="xs:integer"/>
                            <xs:attribute name="sack" use="optional" type="xs:integer"/>
                            <xs:attribute name="ast_sack" use="optional" type="xs:integer"/>
                            <xs:attribute name="sack_yards" use="optional" type="xs:double"/>
                            <xs:attribute name="block" use="optional" type="xs:integer"/>
                            <xs:attribute name="pass_defended" use="optional" type="xs:integer"/>
                            <xs:attribute name="qb_hit" use="optional" type="xs:integer"/>
                            <xs:attribute name="interception" use="optional" type="xs:integer"/>
                            <xs:attribute name="int_yards" use="optional" type="xs:integer"/>
                            <xs:attribute name="int_touchdown" use="optional" type="xs:integer"/>
                            <xs:attribute name="tlost" use="optional" type="xs:integer"/>
                            <xs:attribute name="ast_tlost" use="optional" type="xs:integer"/>
                            <xs:attribute name="tlost_yards" use="optional" type="xs:integer"/>
                            <xs:attribute name="forced_fumble" use="optional" type="xs:integer"/>
                            <xs:attribute name="safety" use="optional" type="xs:integer"/>
                            <xs:attribute name="category" use="optional" type="xs:string"/>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="conversion">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="basePlayStatisticType">
                            <xs:attribute name="attempt" use="optional" type="xs:integer"/>
                            <xs:attribute name="complete" use="optional" type="xs:integer"/>
                            <xs:attribute name="safety" use="optional" type="xs:integer"/>
                            <xs:attribute name="category" use="optional">
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="pass" />
                                        <xs:enumeration value="receive" />
                                        <xs:enumeration value="rush" />
                                        <xs:enumeration value="turnover" />
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:attribute>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
